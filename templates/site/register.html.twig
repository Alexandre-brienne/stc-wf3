<!DOCTYPE html>
<html lang="fr">
	<head>
		<meta charset="UTF-8">
		<meta http-equiv="X-UA-Compatible" content="IE=edge">
		<meta name="viewport" content="width=device-width, initial-scale=1.0">
		{% block title %}
			<title>Inscription</title>
		{% endblock %}
		{% block stylesheets %}
			<link rel="stylesheet" href="CSS/style2.css">
			<link href="https://fonts.googleapis.com/css2?family=Roboto&display=swap" rel="stylesheet">
			<link rel="stylesheet" href="https://maxst.icons8.com/vue-static/landings/line-awesome/line-awesome/1.3.0/css/line-awesome.min.css">
		{% endblock %}

	</head>
	{% block body %}
		<body class="register-body">
			<header class="register-header ">
				<nav class="register-nav">
					<div class="register-brand-logo">
						<a href="#" class='register-a'>
							<i class="fab fa-facebook"></i>
						</a>
						<a href="#" class='register-a'>
							<i class="fab fa-twitter"></i>
						</a>
						<a href="#" class='register-a'>
							<i class="fab fa-instagram"></i>
						</a>
					</div>
					<a href="#" class='register-a'><img src="image/logo_geekandstart.png" alt="logo" class="logo-nav"></a>
					<i id="#connexion-responsive" class="las la-power-off"></i>
				</nav>
				<!-- Formulaire d'inscription -->
				<section id="all">

					<section id="register-principal1" class="principal">
						<section id="register-inscription">

							{{ include('site/_formregister.html.twig') }}

							
							<a class='register-a' id="ButtonContact-Rejoindre" href="#">Nous Contacter</a>
						</section>
					</section>

					<!-- Se Connecter -->

					<section id="register-principal2" class="principal">
						<section id="register-connexion">
							<h1 class="register-h1">Déjà membre?</h1>
							<h1 class="register-h1">Connecte toi.</h1>
						</section>
						<!-- Formulaire Connexion -->
						<section id="register-connexion2">
						
							{{ include('site/_formconnexion.html.twig') }}
							
						</section>
					</section>
				</section>

			</header>
			<script src="https://kit.fontawesome.com/e4895b9a2a.js" crossorigin="anonymous"></script>

			{% block footer %}{% endblock %}

		</body>
	{% endblock %}
	{% block javascripts %}{% endblock %}
</html>
